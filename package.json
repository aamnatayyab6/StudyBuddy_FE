    "react-native-web": "^0.18.12",
    "react-native-webview": "^12.1.0",
    "react-navigation-helpers": "^2.2.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss-react-native": "^1.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.1.2",
    "babel-plugin-react-native-web": "^0.19.4",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "webpack": "^5.81.0"
